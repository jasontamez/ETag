{
	"name": "LocalTag",
	"version": "0.0.1",
	"author": [
		"Jason Tamez"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:LocalTag",
	"descriptionmsg": "localtag-desc",
	"license-name": "MIT",
	"type": "parserhook",
	"AutoloadClasses": {
		"LocalTagHooks": "LocalTag.hooks.php",
		"SpecialLocalTagList": "specials/SpecialLocalTagList.php"
	},
	"config": {
		"LocalTagEnableFoo": true
	},
	"ExtensionMessagesFiles": {
		"LocalTagAlias": "LocalTag.i18n.alias.php"
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"LocalTagHooks::onParserFirstCallInit"
		]
	},
	"MessagesDirs": {
		"LocalTag": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.localTag.foo": {
			"scripts": [],
			"styles": [],
			"messages": [],
			"dependencies": []
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "LocalTag"
	},
	"SpecialPages": {
		"LocalTagList": "SpecialLocalTagList"
	},
	"manifest_version": 1
}
