{
	"name": "ExtraTag",
	"version": "0.0.1",
	"author": [
		"Jason Tamez"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:ExtraTag",
	"descriptionmsg": "extratag-desc",
	"license-name": "MIT",
	"type": "parserhook",
	"AutoloadClasses": {
		"ExtraTagHooks": "ExtraTag.hooks.php",
		"SpecialHelloWorld": "specials/SpecialHelloWorld.php"
	},
	"config": {
		"ExtraTagEnableFoo": true
	},
	"ExtensionMessagesFiles": {
		"ExtraTagAlias": "ExtraTag.i18n.alias.php"
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"ExtraTagHooks::onParserFirstCallInit"
		]
	},
	"MessagesDirs": {
		"ExtraTag": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.extraTag.foo": {
			"scripts": [
				"modules/ext.extraTag.js",
				"modules/ext.extraTag.foo.js"
			],
			"styles": [
				"modules/ext.extraTag.foo.css"
			],
			"messages": [],
			"dependencies": []
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "ExtraTag"
	},
	"SpecialPages": {
		"HelloWorld": "SpecialHelloWorld"
	},
	"manifest_version": 1
}
